{
  "name": "chimi",
  "version": "0.0.8",
  "description": "Validate JavaScript code from your README",
  "bin": {
    "chimi": "./bin/bin.js"
  },
  "scripts": {
    "build": "babel src/ -d lib/ && yarn format",
    "prettier": "prettier --no-semi --single-quote --trailing-comma es5 --parser flow",
    "format": "yarn prettier --write '**/*.js'",
    "flow": "flow",
    "precommit": "yarn check && yarn build && lint-staged",
    "prepublish": "yarn build",
    "release": "np",
    "test": "jest"
  },
  "author": "gillchristian <gillchristiang@gmail.com>",
  "contributors": [
    "fvictorio <victorio.franco@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Huemul/chimi.git"
  },
  "bugs": {
    "url": "https://github.com/Huemul/chimi/issues"
  },
  "homepage": "https://github.com/Huemul/chimi#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "chipa": "^0.1.1",
    "common-tags": "^1.4.0",
    "cosmiconfig": "^3.1.0",
    "fluture": "^7.1.3",
    "fluture-sanctuary-types": "^1.3.0",
    "meow": "^3.7.0",
    "ora": "^1.2.0",
    "pluralize": "^4.0.0",
    "ramda": "^0.23.0",
    "sanctuary": "^0.13.2",
    "sanctuary-def": "^0.13.1",
    "source-map": "^0.6.1",
    "source-map-support": "^0.5.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-flow": "^6.23.0",
    "flow-bin": "^0.56.0",
    "flow-typed": "^2.2.0",
    "husky": "^0.13.3",
    "jest": "^21.2.1",
    "lint-staged": "^3.4.1",
    "np": "^2.14.1",
    "prettier": "^1.3.1"
  },
  "lint-staged": {
    "**/*.js": [
      "yarn prettier --write",
      "git add"
    ]
  }
}
