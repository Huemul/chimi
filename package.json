{
  "name": "chimi",
  "version": "0.0.8",
  "description": "Validate JavaScript code from your README",
  "bin": {
    "chimi": "./bin/bin.js"
  },
  "scripts": {
    "prettier": "prettier --no-semi --single-quote --trailing-comma es5",
    "format": "npm run prettier -- --write '{bin,lib,tests}/**/*.js'",
    "precommit": "lint-staged",
    "release": "np",
    "test": "jest"
  },
  "author": "gillchristian <gillchristiang@gmail.com>",
  "contributors": [
    "fvictorio <victorio.franco@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Huemul/chimi.git"
  },
  "bugs": {
    "url": "https://github.com/Huemul/chimi/issues"
  },
  "homepage": "https://github.com/Huemul/chimi#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "chipa": "^0.1.1",
    "common-tags": "^1.4.0",
    "data.either": "^1.5.0",
    "data.task": "^3.1.1",
    "immutable": "^3.8.1",
    "immutable-ext": "^1.0.8",
    "meow": "^3.7.0",
    "ora": "^1.2.0",
    "pluralize": "^4.0.0",
    "ramda": "^0.23.0"
  },
  "devDependencies": {
    "husky": "^0.13.3",
    "jest": "^21.2.1",
    "lint-staged": "^3.4.1",
    "np": "^2.14.1",
    "prettier": "^1.3.1"
  },
  "lint-staged": {
    "{bin,lib,tests}/**/*.js": [
      "npm run prettier -- --write",
      "git add"
    ]
  }
}
