{
  "name": "chimi",
  "version": "0.0.1",
  "description": "Run JavaScript snippets from your markdown files",
  "bin": {
    "chimi": "./bin/bin.js"
  },
  "scripts": {
    "prettier": "prettier --no-semi --single-quote --trailing-comma es5",
    "format": "npm run prettier -- --write 'lib/**/*.js' && npm run prettier -- --write 'bin/**/*.js'",
    "babel": "babel runner.js -o runner.build.js",
    "precommit": "lint-staged"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^1.1.3",
    "chipa": "^0.1.1",
    "common-tags": "^1.4.0",
    "data.either": "^1.5.0",
    "data.maybe": "^1.2.2",
    "data.task": "^3.1.1",
    "immutable": "^3.8.1",
    "immutable-ext": "^1.0.8",
    "intercept-stdout": "^0.1.2",
    "ora": "^1.2.0",
    "pluralize": "^4.0.0",
    "ramda": "^0.23.0",
    "require-from-string": "^1.2.1",
    "rxjs-es": "^5.0.0-beta.12"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.1",
    "prettier": "^1.3.1"
  },
  "lint-staged": {
    "bin/**/*.js": [
      "npm run prettier -- --write",
      "git add"
    ],
    "lib/**/*.js": [
      "npm run prettier -- --write",
      "git add"
    ]
  }
}
